{
  "name": "developer-platform-install",
  "productName": "Red Hat Developer Platform installer",
  "version": "0.0.1",
  "description": "Installer for Red Hat Developer Platform",
  "main": "main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-developer/developer-platform-install.git"
  },
  "author": "Red Hat Developer Tooling Group",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/redhat-developer/developer-platform-install/issues"
  },
  "homepage": "https://github.com/redhat-developer/developer-platform-install#readme",
  "scripts": {
    "postinstall": "(cd src && npm install && jspm install --yes)",
    "start": "node_modules/.bin/electron ./src",
    "generate": "node_modules/.bin/electron-packager ./src/ DeveloperPlatformInstaller --platform=win32 --arch=x64 --version=0.34.2 --out=./dist/win/ --overwrite --asar",
    "package": "node_modules/.bin/electron-installer-squirrel-windows ./dist/win/DeveloperPlatform-win32-x64 --out=./dist/win/ --name=developer_platform --exe=DeveloperPlatformInstaller.exe --overwrite --authors=\"Red Hat Developer Tooling Group\"",
    "test": "node_modules/.bin/mocha --recursive --compilers js:babel/register"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "del": "^2.0.2",
    "electron-installer-squirrel-windows": "^0.4.0",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.34.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-run": "^1.6.11",
    "jspm": "^0.16.10",
    "rimraf": "^2.4.3",
    "run-sequence": "^1.1.4",
    "tmp": "0.0.28"
  }
}
