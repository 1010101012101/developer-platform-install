{
  "name": "developer-platform-install",
  "productName": "Red Hat Developer Platform installer",
  "version": "0.0.1",
  "description": "Installer for Red Hat Developer Platform",
  "main": "main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-developer/developer-platform-install.git"
  },
  "author": "Red Hat Developer Tooling Group",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/redhat-developer/developer-platform-install/issues"
  },
  "homepage": "https://github.com/redhat-developer/developer-platform-install#readme",
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "package": "node_modules/.bin/electron-packager ./build/ DeveloperPlatform --platform=win32 --arch=x64 --version=0.33.9 --out=./dist/win32_x64/exploded/ --overwrite --asar=true",
    "test": "node_modules/.bin/mocha --recursive --compilers js:babel/register"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "patternfly": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "del": "^2.0.2",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.33.9",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-run": "^1.6.11",
    "jspm": "^0.16.10",
    "rimraf": "^2.4.3",
    "run-sequence": "^1.1.4",
    "tmp": "0.0.28"
  },
  "jspm": {
    "directories": {
      "baseURL": "browser"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.4.6",
      "angular-ui-router": "github:angular-ui/ui-router@^0.2.15",
      "patternfly": "npm:patternfly@^2.2.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
